{
  "parser": "babel-eslint",
  "rules": {
    "consistent-return": 2,
    "curly": [2, "multi-line"],
    "eqeqeq": 2,
    "generator-star-spacing": [1, {
      "before": false,
      "after": true
    }],
    "indent": [2, 2],
    "key-spacing": [2, {
      "beforeColon": false,
      "afterColon": true,
      "mode": "strict"
    }],
    "keyword-spacing": [2, {
      "before": true,
      "after": true,
      "overrides": {}
    }],
    "linebreak-style": [2, "unix"],
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "space-before-function-paren": [2, {
      "anonymous": "always",
      "named": "never"
    }],
    "strict": 0,
    "vars-on-top": 2,
    "yield-star-spacing": [2, {
      "before": false,
      "after": true
    }],
    "no-array-constructor": 2,
    "no-extend-native": 2,
    "no-lone-blocks": 2,
    "no-native-reassign": 2,
    "no-new-object": 2,
    "no-new-wrappers": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-proto": 2,
    "no-param-reassign": 2,
    "no-return-assign": 2,
    "no-sequences": 2,
    "no-trailing-spaces": [2, {
      "skipBlankLines": true
    }],
    "no-undef": 2,
    "no-undef-init": 2,
    "no-undefined": 2,
    "no-unused-vars": 2,
    "no-void": 2,
    "no-with": 2
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "forOf": true,
    "generators": false,
    "modules": true,
    "spread": true,
    "templateStrings": true,
  },
  "plugins": []
}
